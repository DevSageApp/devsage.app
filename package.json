{
  "name": "devsage",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:api": "nodemon --watch server --ext ts --exec \"ts-node server/index.ts\"",
    "dev:all": "powershell -Command \"Start-Process powershell -ArgumentList 'npm run dev:api' ; npm run dev\"",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "lint": "eslint . --ext ts,tsx",
    "test": "vitest run"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.44.0",
    "date-fns": "^3.6.0",
    "dompurify": "^3.1.6",
    "dotenv": "^17.2.3",
    "express": "^4.19.2",
    "immer": "^10.1.1",
    "node-fetch": "^3.3.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-ga4": "^2.1.0",
    "react-router-dom": "^6.26.1",
    "web-vitals": "^4.2.1",
    "zod": "^3.23.8",
    "zustand": "^4.5.3"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.14",
    "@types/node": "^22.5.4",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.11.1",
    "nodemon": "^3.1.0",
    "postcss": "^8.5.6",
    "rollup-plugin-visualizer": "^5.14.0",
    "tailwindcss": "^4.1.14",
    "terser": "^5.44.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.2",
